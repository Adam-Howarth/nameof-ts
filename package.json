{
	"name": "nameof-ts",
	"version": "1.0.3",
	"description": "",
	"main": "index.js",
	"scripts": {
		"tsc": "node_modules/.bin/tsc --project \"./tsconfig.prod.json\"",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"stylelint": "stylelint ./src/** --config ./stylelint.config.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Adam-Howarth/nameof-ts.git"
	},
	"keywords": [
		"nameof",
		"typescript"
	],
	"author": "Adam Howarth",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Adam-Howarth/nameof-ts/issues"
	},
	"homepage": "https://github.com/Adam-Howarth/nameof-ts#readme",
	"files": [
		"package.json",
		"index.js",
		"index.d.ts",
		"src/**/*.js",
		"src/**/*.d.ts"
	],
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@types/jest": "^26.0.15",
		"jest": "^26.5.3",
		"ts-jest": "^26.4.3",
		"ts-node": "8.10.2",
		"tslint": "6.1.2",
		"typescript": "3.9.3"
	},
	"dependencies": {
		"babel-preset-react-app": "^10.0.0"
	},
	"jest": {
		"roots": [
			"<rootDir>/src",
			"<rootDir>/test"
		],
		"testMatch": [
			"<rootDir>/**/*.test.{ts,tsx}"
		],
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
		],
		"moduleFileExtensions": [
			"js",
			"ts"
		]
	},
	"babel": {
		"presets": [
			"react-app"
		]
	}
}
